    {
      "name": "dgb -ex r python --args demo_dgbullet_pybullet_quadruped.py",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/python",
      "args": ["${workspaceFolder}/catkin/dg_control/dg_bullet/demos/demo_dgbullet_pybullet_quadruped.py",],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
            "description": "enable the python debugging support",
            "text": "-ex r",
            "ignoreFailures": true
        }
      ],
    },
    {
      "name": "Python: demo_dgbullet_pybullet_quadruped.py",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/catkin/dg_control/dg_bullet/demos/demo_dgbullet_pybullet_quadruped.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "dgb -ex r python --args [current file].py",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/python",
      "args": ["${file}",],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        },
        {
          "description": "enable the python debugging support",
          "text": "-ex r",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "Python: test meshes gepetto_gui_visualization.py",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/catkin/robots/robot_properties/robot_properties_test_meshes/nodes/gepetto_gui_visualization",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Python: quadruped gepetto_gui_visualization.py",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/catkin/robots/robot_properties/robot_properties_quadruped/nodes/gepetto_gui_visualization",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Python: vicon quadruped demo",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/catkin/dg_control/dg_vicon_sdk/demos/demo_quadruped_pose_vel_vicon.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Python: Current File in py_nmpc_multicontact(Integrated Terminal)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/catkin/humanoids/nmpc_multicontact/nmpc_multicontact_python/"
    },
    {
      "name": "Python: Current File (Integrated Terminal)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Attach",
      "type": "python",
      "request": "attach",
      "port": 5678,
      "host": "localhost"
    },
    {
      "name": "Python: Module",
      "type": "python",
      "request": "launch",
      "module": "enter-your-module-name-here",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "console": "integratedTerminal",
      "args": [
        "runserver",
        "--noreload",
        "--nothreading"
      ],
      "django": true
    },
    {
      "name": "Python: Flask",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "app.py"
      },
      "args": [
        "run",
        "--no-debugger",
        "--no-reload"
      ],
      "jinja": true
    },
    {
      "name": "Python: Current File (External Terminal)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "externalTerminal"
    }
  ]
}